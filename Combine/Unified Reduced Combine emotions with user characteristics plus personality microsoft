pt>\n","    async function quickchart(key) {\n","      const quickchartButtonEl =\n","        document.querySelector('#' + key + ' button');\n","      quickchartButtonEl.disabled = true;  // To prevent multiple clicks.\n","      quickchartButtonEl.classList.add('colab-df-spinner');\n","      try {\n","        const charts = await google.colab.kernel.invokeFunction(\n","            'suggestCharts', [key], {});\n","      } catch (error) {\n","        console.error('Error during call to suggestCharts:', error);\n","      }\n","      quickchartButtonEl.classList.remove('colab-df-spinner');\n","      quickchartButtonEl.classList.add('colab-df-quickchart-complete');\n","    }\n","    (() => {\n","      let quickchartButtonEl =\n","        document.querySelector('#df-01c2a285-5961-4a74-890b-2e6ec7b60334 button');\n","      quickchartButtonEl.style.display =\n","        google.colab.kernel.accessAllowed ? 'block' : 'none';\n","    })();\n","  </script>\n","</div>\n","    </div>\n","  </div>\n"],"application/vnd.google.colaboratory.intrinsic+json":{"type":"dataframe","variable_name":"author_train_personality"}},"metadata":{},"execution_count":4}]},{"cell_type":"code","source":["train_emotion"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":424},"id":"RK30LLJ2DkG_","executionInfo":{"status":"ok","timestamp":1715012271930,"user_tz":-330,"elapsed":489,"user":{"displayName":"Thushalya Weerasuriya","userId":"01641029838740269022"}},"outputId":"99c6e738-0614-4239-ac73-b0b2e3b7b63f"},"execution_count":5,"outputs":[{"output_type":"execute_result","data":{"text/plain":["      dataset                                               text  labels  \\\n","0           a  If there is a heaven and if I'm going (not som...       0   \n","1           d  eat shit you boot licking booger eating nazi l...       1   \n","2           a  I hope that kid doesn't get anything good in l...       0   \n","3           n  Britain: (While colonizing the world) If We Do...       1   \n","